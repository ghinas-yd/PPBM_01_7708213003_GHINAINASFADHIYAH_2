<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuActivity"
    android:background="@drawable/mainmenu">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/videografi"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginStart="170dp"
            android:layout_marginTop="450dp"
            android:background="@drawable/video"
            tools:ignore="SpeakableTextPresentCheck">

        </androidx.appcompat.widget.AppCompatButton>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/postinstagram"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginHorizontal="360sp"
            android:layout_marginTop="25dp"
            android:background="@drawable/add"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck">

        </androidx.appcompat.widget.AppCompatButton>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/back"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginHorizontal="10dp"
            android:layout_marginTop="25dp"
            android:background="@drawable/baseline_arrow_back_24"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck">

        </androidx.appcompat.widget.AppCompatButton>


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/dua"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginLeft="65dp"
            android:layout_marginTop="450dp"
            android:background="@drawable/duadimensi"
            tools:ignore="RtlHardcoded,SpeakableTextPresentCheck">

        </androidx.appcompat.widget.AppCompatButton>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/tigadimensi"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_alignParentRight="true"
            android:layout_marginTop="450dp"
            android:layout_marginEnd="60dp"
            android:background="@drawable/tigademensi"
            tools:ignore="RtlHardcoded,SpeakableTextPresentCheck">

        </androidx.appcompat.widget.AppCompatButton>

        <TextView
            android:layout_width="81dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="300dp"
            android:layout_below="@+id/tigadimensi"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="40dp"
            android:fontFamily="@font/lexend"
            android:text="3D"
            android:textSize="20sp"
            tools:ignore="HardcodedText,TextSizeCheck">

        </TextView>

        <TextView
            android:layout_below="@+id/dua"
            android:layout_width="81dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="90dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="40dp"
            android:fontFamily="@font/lexend"
            android:text="2D"
            android:textSize="20sp"
            tools:ignore="HardcodedText,TextSizeCheck">

        </TextView>

        <TextView
            android:layout_width="111dp"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="40dp"
            android:fontFamily="@font/lexend"
            android:layout_below="@+id/videografi"
            android:text="videografi"
            android:textSize="20sp"
            tools:ignore="HardcodedText,TextSizeCheck">

        </TextView>
        <RelativeLayout
            android:layout_width="350dp"
            android:layout_height="100dp"
            android:layout_marginTop="750dp"
            android:layout_centerHorizontal="true">

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/home"
                    android:layout_width="66dp"
                    android:layout_height="66dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/beranda"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"/>

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/settings"
                    android:layout_width="63dp"
                    android:layout_height="63dp"
                    android:layout_toStartOf="@id/home"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="60dp"
                    android:background="@drawable/setting"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"/>

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/listharga"
                    android:layout_width="62dp"
                    android:layout_height="65dp"
                    android:layout_marginStart="275dp"
                    android:layout_marginTop="14dp"
                    android:background="@drawable/menu"
                    tools:ignore="NotSibling,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        </RelativeLayout>





</RelativeLayout>
